ems=\"center\"\n          borderRadius=\".5rem 0 0 .5rem\"\n        />\n        <Box\n          w=\"65%\"\n          h=\"100%\"\n          bg={`url(${item?.poster || item?.banner})`}\n          bgSize=\"cover\"\n          borderRadius=\"0 .5rem .5rem 0\"\n        />\n      </Flex>\n      <Flex\n        pos=\"absolute\"\n        top=\"0\"\n        left=\"0\"\n        right=\"0\"\n        bottom=\"0\"\n        pl=\"5rem\"\n        color=\"#fff\"\n        align=\"center\"\n      >\n        <Box>\n          <Text\n            fontFamily=\"ReformaGroteskDemiC\"\n            fontSize=\"7rem\"\n            fontWeight=\"700\"\n          >\n            {item.name}\n          </Text>\n          <Text maxW=\"500px\">{truncate(item.description, 250)}</Text>\n          <Flex align=\"center\" mt=\"2rem\">\n          <HStack>\n                  <Icon color=\"#fff\" as={BiCart} />\n                  <Text color=\"#fff\" fontSize=\"1.2rem\">\n                    {country === 'Nigeria' ? '₦' : '$'}\n                    {country === 'Nigeria'\n                      ? commafy(item.price * 470)\n                      : item.price}\n                  </Text>\n                </HStack>\n            {/* {item.type === 'single' ? (\n              <>\n                <HStack>\n                  <Icon color=\"#fff\" as={BiCart} />\n                  <Text color=\"#fff\" fontSize=\"1.2rem\">\n                    {country === 'Nigeria' ? '₦' : '$'}\n                    {country === 'Nigeria'\n                      ? commafy(item.price * 470)\n                      : item.price}\n                  </Text>\n                </HStack>\n              </>\n            ) : (\n              <HStack>\n                <Icon color=\"#fff\" as={RiMovieLine} />\n                <Text color=\"#fff\" fontSize=\"1.2rem\">\n                  {item.seasons?.length > 1\n                      ? `${item.seasons?.length} Seasons`\n                      : `${item.seasons?.length} Season`\n                   }\n                </Text>\n              </HStack>\n            )} */}\n            <Box color=\"#fff\" fontWeight=\"800\" fontSize=\"1.2rem\" mx=\".5rem\">\n              .\n            </Box>\n            <HStack>\n              <Icon color=\"#fff\" as={BsClockHistory} />\n              <Text color=\"#fff\" fontSize=\"1.2rem\">\n                {item.runtime || '45min'}\n              </Text>\n            </HStack>\n            <Box color=\"#fff\" fontWeight=\"800\" fontSize=\"1.2rem\" mx=\".5rem\">\n              .\n            </Box>\n            <Text color=\"#fff\" fontSize=\"1.2rem\">\n              {item.year}\n            </Text>\n          </Flex>\n          <HStack spacing=\"3rem\" mt=\"2rem\">\n            <Btn\n              onClick={handleClick}\n              p=\"2rem\"\n              fontSize=\"1.2rem\"\n              leftIcon={<AiOutlinePlayCircle />}\n            >\n              Watch now\n            </Btn>\n            {/* <IconButton\n              borderColor=\"#fff\"\n              variant=\"outline\"\n              color=\"#fff\"\n              aria-label=\"Send email\"\n              p=\"2rem 1rem\"\n              icon={<AiOutlineHeart fontSize=\"2rem\" />}\n            /> */}\n          </HStack>\n        </Box>\n      </Flex>\n    </Box>\n  );\n}\n\nconst Left = styled(Flex)`\n  &:before {\n    content: '';\n    position: absolute;\n    background-image: linear-gradient(to right, #14142b, transparent);\n    top: 0;\n    bottom: 0;\n    left: 100%;\n    width: 275px;\n  }\n`;\n\nfunction Banner({ bannerData }) {\n  const [isLargerThan480] = useMediaQuery('(min-width: 480px)');\n\n  const sliderRef = useRef();\n\n  const SlderNav = chakra(NextIcon);\n\n  const settings = {\n    dots: false,\n    infinite: true,\n    speed: 500,\n    slidesToShow: 1,\n    slidesToScroll: 1,\n  };\n\n  const nextSlide = () => sliderRef.current.slickNext();\n\n  return (\n    <>\n      {isLargerThan480 ? (\n        <Box pos=\"relative\" mb=\"6rem\">\n          <Slider