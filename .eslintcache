const { colorMode } = useColorMode()\n\n  const handleClick = async () => {\n    if (!authState?.jwt) {\n      history.push('/login')\n      return\n    }\n\n    const isOwner = await checkIfUserOwnsItem(\n      authState.jwt,\n      authState.user.id,\n      item.id\n    )\n\n    if (isOwner) {\n      history.push('/player')\n    } else {\n      onToggle()\n    }\n  }\n\n  return (\n    <>\n      {isLargerThan480 ? (\n        <Box w=\"100%\" h={['400px', '500px']} pos=\"relative\">\n          <Flex h=\"100%\">\n            <Left\n              pos=\"relative\"\n              bgColor=\"#000\"\n              w=\"35%\"\n              h=\"100%\"\n              alignItems=\"center\"\n              borderRadius=\".5rem 0 0 .5rem\"\n            />\n            <Box\n              w=\"65%\"\n              h=\"100%\"\n              bg={`url(${item.banner})`}\n              bgSize=\"cover\"\n              borderRadius=\"0 .5rem .5rem 0\"\n            ></Box>\n          </Flex>\n          <Box\n            pos=\"absolute\"\n            top=\"0\"\n            left=\"0\"\n            right=\"0\"\n            bottom=\"0\"\n            pt=\"7rem\"\n            pl={['2rem', '5rem']}\n            color=\"#fff\"\n          >\n            <Text\n              fontFamily=\"ReformaGroteskDemiC\"\n              fontSize={['5rem', '7rem']}\n              fontWeight=\"700\"\n            >\n              {item.title}\n            </Text>\n            <Text maxW=\"500px\" fontSize={['1.2rem', null]}>\n              {truncate(item.description, 250)}\n            </Text>\n            <Flex align=\"center\" mt=\"2rem\">\n            <HStack>\n                    <Icon color=\"#fff\" as={BiCart} />\n                    <Text color=\"#fff\" fontSize=\"1.2rem\">\n                      {country === 'Nigeria' ? '₦' : '$'}\n                      {country === 'Nigeria'\n                        ? commafy(item.price * 470)\n                        : item.price}\n                    </Text>\n           